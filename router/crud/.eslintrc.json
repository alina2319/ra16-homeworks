{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-tabs": 0, // Убираем проверку на tab в качестве отступов.
    "indent": ["error", "tab"] // Убираем проверку на tab в качестве отступов.
  }
}
